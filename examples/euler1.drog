(
Do

  (Func sum-divisible-by upper value)
    (Do
      (Let x-over-n (Send 'round (/ upper value)))
      (Let x-over-n-inc (+ 1 x-over-n))

      ( / ( * ( * value x-over-n ) x-over-n-inc) 2 )
    )

  (Let sum-divisible-five (Call sum-divisible-by 999 5))
  (Let sum-divisible-three (Call sum-divisible-by 999 3))
  (Let sum-divisible-fifteen (Call sum-divisible-by 999 15))

  (Let ans (- (+ sum-divisible-three sum-divisible-five) sum-divisible-fifteen))
  (Show ans)

)
